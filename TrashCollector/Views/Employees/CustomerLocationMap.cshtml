@model TrashCollector.Models.GeoCoderToFindCustomerLocation

@{
    ViewBag.Title = "CustomerLocationMap";
}
<h1>Customer Location</h1>
<html>
<head>
    <style>
        #map {
            height: 400px;
            width: 100%;
        }
    </style>
</head>
<body>
    <h3>My Google Maps Demo</h3>
    <div id="map"></div>
    <script>
        function initMap(Model model) {
            var wisconsin = { lat: latitude, lng: longitude };

            var map = new google.maps.Map(
            document.getElementById('map'), { zoom: 4, center: wisconsin });

            var marker = new google.maps.Marker({ position: wisconsin, map: map });
            }
            </script>
            <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=&callback=initMap"
            </body>
            </html>
